{
  "name": "malteditor",
  "version": "0.0.1",
  "description": "Hypersimple text editor",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "lz-string": "^1.4.4",
    "parcel-bundler": "1.12.3",
    "parcel-plugin-sw-cache": "^0.3.1"
  },
  "devDependencies": {
    "sass": "^1.32.8"
  },
  "cache": {
    "strategy": "inject",
    "globPatterns": [
      "**/*.{html,js,css,jpg,png,webmanifest,ico,svg}"
    ],
    "swSrc": "./src/sw.js",
    "swDest": "sw.js"
  }
}
